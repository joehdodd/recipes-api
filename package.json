{
  "name": "Recipes-API",
  "version": "1.0.0",
  "description": "Node API for Recipes app.",
  "main": "index.js",
  "repository": "https://github.com/joehdodd/recipes-api.git",
  "author": "Joe Dodd",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon src/index.js --exec babel-node",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js"
  },
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "regenerator-runtime": "^0.13.2",
    "sequelize": "^5.15.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0"
  }
}
